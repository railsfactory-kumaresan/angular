<!DOCTYPE html>
<html>
	<head>
	  <meta charset="utf-8" />

		<title>Inquirly</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

		<!--IE Support-->
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="discription" content="">
		<meta name="keywords" content="">
			<!-- rails4_image_tag_impact -->
		<%= favicon_link_tag 'responsive/favicon.ico', rel: 'shortcut icon', type: 'image/x-icon'%>
		<%= favicon_link_tag 'responsive/favicon.ico', rel: 'icon', type: 'image/x-icon'%>
		<!--<link rel="shortcut icon" href="/assets/responsive/favicon.ico" type="image/x-icon">
		<link rel="icon" href="/assets/responsive/favicon.ico" type="image/x-icon">-->
		<!-- Bootstrap -->
		<!--link href="stylesheets/common/bootstrap.min.css" rel="stylesheet" media="screen">
		<link href="stylesheets/common/jquery-ui-datepicker.css" rel="stylesheet" media="screen">
		<link href="stylesheets/style.css" rel="stylesheet" media="screen">
		<link href="stylesheets/mediaqueries.css" rel="stylesheet" media="screen"-->
		<!--css-->
		<% a=controller.action_name %>
		<% b=controller.controller_name %>
		<%= stylesheet_link_tag    "application", :media => "all" %>
		<%= javascript_include_tag "application" %>
		<%= yield :page_specific_head_content %>
		<%= csrf_meta_tags %>

		<!--// start typekit font include -->
		<script type="text/javascript" src="//use.typekit.net/ltc1ytn.js"></script>
		<script type="text/javascript">try{Typekit.load();}catch(e){}</script>
		<!--// end typekit font include -->

	</head>
	<body>
		<div class="header navbar navbar-inverse bs-docs-nav navbar-fixed-top">
			<div class="container">
				<div class="navbar-header">
					<button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".bs-navbar-collapse">
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
					<a href="/" class="navbar-brand">
					<!-- rails4_image_tag_impact -->
					<%= image_tag("responsive/logo-inquirly.png", alt: "Inquirly") %></a>
				</div>
			</div>
		</div>

		<div class="breadcrumb-content">
			<div class="container">
			</div>
		</div>
		<% benchmark '##Render flash message in forgotpassword' do %>
     <%= render :partial => "shared/flash_message"%>
     <% end %>
    <%= yield %>
    <div class="footer">
		<% benchmark '##Render footer in forgotpassword' do %>
    <%= render "shared/footer" %>
    <% end %>

    </div>
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
